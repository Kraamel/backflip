<form action="{{formAction}}" method="post" class="signin-form pure-form">
  <p>{{{__ "An invitation to join the private beta."}}}</p>
  <input class="pure-input-1 pure-input-rounded" type="email" name="email" value="{{email}}" placeholder="{{__ "Your work email"}}">
  <input id="jeSuisHumain" class="pure-input-1" type="text" name="jeSuisHumain">
  {{#errors}}<p class="errors"><i class="fa fa-hand-paper-o" aria-hidden="true"></i> {{msg}}</p>{{/errors}}
  <button type="submit" class="pure-button">{{__ "Get Started"}}</button>
  <p class="homepage-or">{{__ "or"}}</p>
  <a class="google-signin pure-button" href="{{googleSigninUrl}}"><img src="/images/google.svg"> {{__ "Get Started with Google"}}</a>
</form>
<script>
  window.setTimeout(function() {
    document.getElementById('jeSuisHumain').value = 'Oui!';
  },1000);
</script>

<script>
  $('.edit .radio-buttons label').click(function () {
    $(this).parent().children('label').removeClass('checked');
    $(this).addClass('checked');
  });
  $('#type-radio input:checked').parent().addClass('checked');
</script>
<script>
  $('#type-radio label').click(function () {
    $('form').removeClass('person').removeClass('team').removeClass('hashtag');
    $('form').addClass(this.getAttribute('for'));
    $('#formBody').removeClass('formBody-hidden');
  });
</script>
